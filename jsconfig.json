{
  "compilerOptions": {
    "target": "esnext",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@core/*": ["src/core/*"],
      "@modules/*": ["src/core/modules/*"],
      "@script/*": ["src/script/*]"
    }
  },
  "exclude": ["node_modules"],
}
